# Core DRF & Django errors
msgid "Unknown error."
msgstr "Error desconocido."

msgid "Not authenticated."
msgstr "No autenticado."

msgid "Authentication credentials were not provided."
msgstr "No se proporcionaron credenciales de autenticación."

msgid "You do not have permission to perform this action."
msgstr "No tienes permiso para realizar esta acción."

msgid "Not found."
msgstr "No encontrado."

msgid "A server error occurred."
msgstr "Se produjo un error del servidor."

msgid "Invalid input."
msgstr "Entrada no válida."

msgid "Malformed request."
msgstr "Solicitud malformada."

msgid "Unsupported media type."
msgstr "Tipo de medio no admitido."

msgid "Not acceptable."
msgstr "No aceptable."

msgid "Request was throttled."
msgstr "Se ha limitado la solicitud."

msgid "Method \"%(method)s\" not allowed."
msgstr "Método \"%(method)s\" no permitido."

# Your custom messages (examples already present)
msgid "ids must be a list of integers"
msgstr "ids debe ser una lista de enteros"

msgid "Cannot delete current user."
msgstr "No se puede eliminar al usuario actual."

msgid "Passwords do not match."
msgstr "Las contraseñas no coinciden."

# SimpleJWT / auth-ish
msgid "Invalid token."
msgstr "Token no válido."

msgid "Token is invalid or expired"
msgstr "El token no es válido o ha expirado"

msgid "Given token not valid for any token type"
msgstr "El token proporcionado no es válido para ningún tipo de token"

msgid "User is inactive"
msgstr "El usuario está inactivo"

msgid "No active account found with the given credentials"
msgstr "No se encontró ninguna cuenta activa con las credenciales proporcionadas"

msgid "No refresh token provided"
msgstr "No se proporcionó el token de actualización"

msgid "Session expired due to server restart."
msgstr "La sesión expiró debido al reinicio del servidor."

msgid "Invalid refresh token."
msgstr "Token de actualización no válido."

msgid "Failed to blacklist token."
msgstr "No se pudo incluir el token en la lista negra."

msgid "Database error while applying changes."
msgstr "Error de base de datos al aplicar los cambios."

msgid "Password updated."
msgstr "Contraseña actualizada."

msgid "No file provided"
msgstr "No se proporcionó ningún archivo"

msgid "Must include 'login' (or 'email'/'username') and 'password'."
msgstr "Debe incluir 'login' (o 'email'/'username') y 'password'."

msgid "Email required"
msgstr "Correo electrónico requerido"

msgid "user_id claim missing"
msgstr "Falta el atributo 'user_id'"

msgid "Session expired due to inactivity."
msgstr "La sesión expiró por inactividad."

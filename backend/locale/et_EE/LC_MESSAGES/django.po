# Core DRF & Django errors
msgid "Unknown error."
msgstr "Tundmatu viga."

msgid "Not authenticated."
msgstr "Pole autentitud."

msgid "Authentication credentials were not provided."
msgstr "Autentimisandmeid ei esitatud."

msgid "You do not have permission to perform this action."
msgstr "Sul pole selle toimingu õigust."

msgid "Not found."
msgstr "Ei leitud."

msgid "A server error occurred."
msgstr "Tekkis serveripoolne viga."

msgid "Invalid input."
msgstr "Vigane sisend."

msgid "Malformed request."
msgstr "Vigase vorminguga päring."

msgid "Unsupported media type."
msgstr "Mitterahuldatud meediatüüp."

msgid "Not acceptable."
msgstr "Ei ole vastuvõetav."

msgid "Request was throttled."
msgstr "Päringute arvu on piiratud."

msgid "Method \"%(method)s\" not allowed."
msgstr "Meetod \"%(method)s\" pole lubatud."

# Your custom messages (examples already present)
msgid "ids must be a list of integers"
msgstr "ids peab olema täisarvude loend"

msgid "Cannot delete current user."
msgstr "Praegust kasutajat ei saa kustutada."

msgid "Passwords do not match."
msgstr "Paroolid ei ühti."

# SimpleJWT / auth-ish
msgid "Invalid token."
msgstr "Vigane tunnus."

msgid "Token is invalid or expired"
msgstr "Tunnus on vigane või aegunud."

msgid "Given token not valid for any token type"
msgstr "Antud tunnus ei sobi ühegi tüübi jaoks."

msgid "User is inactive"
msgstr "Kasutaja on mitteaktiivne."

msgid "No active account found with the given credentials"
msgstr "Antud andmetega aktiivset kontot ei leitud."

msgid "No refresh token provided"
msgstr "Värskendustokenit ei ole esitatud"

msgid "Session expired due to server restart."
msgstr "Seanss aegus serveri taaskäivitamise tõttu."

msgid "Invalid refresh token."
msgstr "Kehtetu värskendustoken."

msgid "Failed to blacklist token."
msgstr "Tokeni musta nimekirja lisamine ebaõnnestus."

msgid "Database error while applying changes."
msgstr "Andmebaasi viga muudatuste rakendamisel."

msgid "Password updated."
msgstr "Parool uuendatud."

msgid "No file provided"
msgstr "Faili ei esitatud"

msgid "Must include 'login' (or 'email'/'username') and 'password'."
msgstr "Tuleb esitada 'login' (või 'email'/'username') ja 'password'."

msgid "Email required"
msgstr "E-posti aadress on nõutav"

msgid "user_id claim missing"
msgstr "user_id nõue puudub"

msgid "Session expired due to inactivity."
msgstr "Seanss aegus tegevusetuse tõttu."

msgid "This password is too short. It must contain at least 8 characters."
msgstr "See parool on liiga lühike. See peab sisaldama vähemalt 8 märki."

msgid "The password is too similar to the %(verbose_name)s."
msgstr "Parool on kasutajanimega liiga %(verbose_name)s."

msgid "This password is too common."
msgstr "See parool on liiga levinud."

msgid "Enter a valid email address."
msgstr "Sisestage kehtiv e-posti aadress."

msgid "No User matches the given query."
msgstr "Ükski kasutaja ei vasta antud päringule."

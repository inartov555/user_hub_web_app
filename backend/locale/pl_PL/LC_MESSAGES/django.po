# Core DRF & Django errors
msgid "Unknown error."
msgstr "Nieznany błąd."

msgid "Not authenticated."
msgstr "Nie uwierzytelniono."

msgid "Authentication credentials were not provided."
msgstr "Nie podano danych uwierzytelniających."

msgid "You do not have permission to perform this action."
msgstr "Nie masz uprawnień do wykonania tej akcji."

msgid "Not found."
msgstr "Nie znaleziono."

msgid "A server error occurred."
msgstr "Wystąpił błąd serwera."

msgid "Invalid input."
msgstr "Nieprawidłowe dane wejściowe."

msgid "Malformed request."
msgstr "Nieprawidłowe żądanie."

msgid "Unsupported media type."
msgstr "Nieobsługiwany typ mediów."

msgid "Not acceptable."
msgstr "Nieakceptowalne."

msgid "Request was throttled."
msgstr "Przekroczono limit zapytań."

msgid "Method \"%(method)s\" not allowed."
msgstr "Metoda \"%(method)s\" jest niedozwolona."

# Your custom messages (examples already present)
msgid "ids must be a list of integers"
msgstr "ids must be a list of integers"

msgid "Cannot delete current user."
msgstr "Nie można usunąć bieżącego użytkownika."

msgid "Passwords do not match."
msgstr "Hasła się nie zgadzają."

# SimpleJWT / auth-ish
msgid "Invalid token."
msgstr "Nieprawidłowy token."

msgid "Token is invalid or expired"
msgstr "Token jest nieprawidłowy lub wygasł"

msgid "Given token not valid for any token type"
msgstr "Podany token nie jest prawidłowy dla żadnego typu tokenu"

msgid "User is inactive"
msgstr "Użytkownik jest nieaktywny"

msgid "No active account found with the given credentials"
msgstr "Nie znaleziono aktywnego konta dla podanych danych logowania"

msgid "No refresh token provided"
msgstr "Nie podano tokenu odświeżającego"

msgid "Session expired due to server restart."
msgstr "Sesja wygasła z powodu ponownego uruchomienia serwera."

msgid "Invalid refresh token."
msgstr "Nieprawidłowy token odświeżający."

msgid "Failed to blacklist token."
msgstr "Nie udało się dodać tokenu do czarnej listy."

msgid "Database error while applying changes."
msgstr "Błąd bazy danych podczas wprowadzania zmian."

msgid "Password updated."
msgstr "Hasło zaktualizowane."

msgid "No file provided"
msgstr "Nie podano pliku"

msgid "Must include 'login' (or 'email'/'username') and 'password'."
msgstr "Należy podać 'login' (lub 'email'/'username') oraz 'password'."

msgid "Email required"
msgstr "Wymagany adres e-mail"

msgid "user_id claim missing"
msgstr "Brak atrybutu 'user_id'"

msgid "Session expired due to inactivity."
msgstr "Sesja wygasła z powodu braku aktywności."

msgid "This password is too short. It must contain at least 8 characters."
msgstr "To hasło jest zbyt krótkie. Musi zawierać co najmniej 8 znaków."

msgid "The password is too similar to the %(verbose_name)s."
msgstr "Hasło jest zbyt podobne do nazwy %(verbose_name)s."

msgid "This password is too common."
msgstr "To hasło jest zbyt powszechne."

msgid "Enter a valid email address."
msgstr "Wprowadź prawidłowy adres e-mail."

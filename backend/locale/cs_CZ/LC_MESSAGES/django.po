# Core DRF & Django errors
msgid "Unknown error."
msgstr "Neznámá chyba."

msgid "Not authenticated."
msgstr "Neautentizováno."

msgid "Authentication credentials were not provided."
msgstr "Nebyly poskytnuty přihlašovací údaje."

msgid "You do not have permission to perform this action."
msgstr "Nemáte oprávnění k provedení této akce."

msgid "Not found."
msgstr "Nenalezeno."

msgid "A server error occurred."
msgstr "Došlo k chybě serveru."

msgid "Invalid input."
msgstr "Neplatný vstup."

msgid "Malformed request."
msgstr "Chybný požadavek."

msgid "Unsupported media type."
msgstr "Nepodporovaný typ média."

msgid "Not acceptable."
msgstr "Nepřijatelné."

msgid "Request was throttled."
msgstr "Požadavek byl omezen."

msgid "Method \"%(method)s\" not allowed."
msgstr "Metoda \"%(method)s\" není povolena."

# Your custom messages (examples already present)
msgid "ids must be a list of integers"
msgstr "ids musí být seznam celých čísel"

msgid "Cannot delete current user."
msgstr "Aktuálního uživatele nelze smazat."

msgid "Passwords do not match."
msgstr "Hesla se neshodují."

# SimpleJWT / auth-ish
msgid "Invalid token."
msgstr "Neplatný token."

msgid "Token is invalid or expired"
msgstr "Token je neplatný nebo vypršel"

msgid "Given token not valid for any token type"
msgstr "Zadaný token není platný pro žádný typ tokenu"

msgid "User is inactive"
msgstr "Uživatel je neaktivní"

msgid "No active account found with the given credentials"
msgstr "Pro zadané údaje nebyl nalezen žádný aktivní účet"

msgid "No refresh token provided"
msgstr "Nebyl poskytnut obnovovací token"

msgid "Session expired due to server restart."
msgstr "Relace vypršela kvůli restartu serveru."

msgid "Invalid refresh token."
msgstr "Neplatný obnovovací token."

msgid "Failed to blacklist token."
msgstr "Token se nepodařilo přidat na černou listinu."

msgid "Database error while applying changes."
msgstr "Chyba databáze při provádění změn."

msgid "Password updated."
msgstr "Heslo aktualizováno."

msgid "No file provided"
msgstr "Nebyl poskytnut žádný soubor"

msgid "Must include 'login' (or 'email'/'username') and 'password'."
msgstr "Musí obsahovat 'login' (nebo 'email'/'username') a 'password'."

msgid "Email required"
msgstr "E-mail je povinný"

msgid "user_id claim missing"
msgstr "Chybí claim user_id"

msgid "Session expired due to inactivity."
msgstr "Relace vypršela z důvodu nečinnosti."
